<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <connectionStrings>
  </connectionStrings>
  <appSettings>
    <add key="PriorityURL" value="https://www.eshbelsaas.com/demo1/odata/Priority/tabdem1.ini,3/comp/" />
	<add key="PriorityUser" value="api" />
    <add key="PriorityPwd" value="123456" />
	<add key="Campaigncodeexist" value="100" />
	<add key="Campaigncodenew" value="100" />
	<add key="Emailsuffix" value="@priority-software.com" />
	<add key="Popupurl" value="https://priority-connect.online/webui/openmail.htm?tenant=priorityAMPERSANDpriority:%20priform#FORMID:%20ROWID:%20comp:%20tabdem1.ini" />
  </appSettings>
  <log4net>
    <root>
      <level value="DEBUG" />
      <appender-ref ref="RollingFileAppender" />
    </root>
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="C:\Logs\RESTApiPriority\logRESTApi_%property{log4net:HostName}_.log" />
      <appendToFile value="true" />
      <rollingStyle value="Composite" />
      <maxSizeRollBackups value="15" />
      <maximumFileSize value="20MB" />
      <staticLogFileName value="false" />
      <preserveLogFileNameExtension value="true" />
      <datePattern value="yyyyMMdd" />
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="DEBUG" />
        <levelMax value="FATAL" />
      </filter>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] [%logger.%method] %level - %message%newline%exception" />
      </layout>
    </appender>
  </log4net>
